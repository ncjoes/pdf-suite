{
    "name" : "ncjoes/pdf-suite",
    "type" : "library",
    "description" : "A high level wrapper over Poppler-Php for PDF content extraction and conversion using Poppler utils",
    "keywords" : [
        "Poppler-utils",
        "PDF Converter",
        "PDF",
        "PDF to HTML Converter",
        "PDF to SVG Converter",
        "PDF to JPG Converter",
        "PDF to PNG Converter",
        "PDF to PostScript Converter"
    ],
    "homepage" : "https://github.com/ncjeos/pdf-suite",
    "license" : "MIT",
    "authors" : [
        {
            "name" : "Chukwuemeka Nwobodo",
            "email" : "jc.nwobodo@gmail.com"
        }
    ],
    "require" : {
        "php" : ">=5.5.9",
        "ncjoes/poppler-php" : "0.1.*",
        "illuminate/filesystem" : "^5.3",
        "illuminate/support" : "^5.3",
        "symfony/dom-crawler" : "3.1.*",
        "symfony/css-selector" : "3.1.*"
    },
    "require-dev" : {
        "phpunit/phpunit" : "5.5.*"
    },
    "autoload" : {
        "psr-4" : {
            "NcJoes\\PdfSuite\\" : "src/PdfSuite",
            "NcJoes\\PdfSuite\\Tests\\" : "tests"
        }
    },
    "minimum-stability" : "stable"
}
